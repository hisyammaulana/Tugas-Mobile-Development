<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.noobs.layoutingapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconku"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScrollActivity"></activity>
        <activity android:name=".ConstraintActivity" />
        <activity android:name=".FrameActivity" />
        <activity android:name=".RelativeActivity" />
        <activity android:name=".LinearActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>